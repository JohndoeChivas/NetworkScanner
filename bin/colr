#!/home/johndoe/Bureau/JohnDoeDev/Network/myenv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'Colr==0.9.1','console_scripts','colr'
__requires__ = 'Colr==0.9.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Colr==0.9.1', 'console_scripts', 'colr')()
    )
